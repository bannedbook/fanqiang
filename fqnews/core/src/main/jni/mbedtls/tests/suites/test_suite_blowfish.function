                                                        sizeof( buf ),
                                                        &off,
                                                        buf, buf,
                                                        buf, buf ) );
    TEST_INVALID_PARAM_RET( MBEDTLS_ERR_BLOWFISH_BAD_INPUT_DATA,
                            mbedtls_blowfish_crypt_ctr( &ctx,
